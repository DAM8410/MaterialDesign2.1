tions</string>\r\n    <string name=\"donate\">Donate</string>\r\n    <string name=\"donated\">Donated</string>\r\n    <string name=\"donate_title\">I Would Be Very Grateful If You Could Donate Me ! </string>\r\n    <string name=\"donate_title_after\">Thanks For Your Donations, I Am Very Grateful ! </string>\r\n    <string name=\"donate_low\">1.99 USD</string>\r\n    <string name=\"donate_high\">9.99 USD</string>\r\n    <string name=\"donate_exception\">Sorry, Failed!</string>\r\n</resources>\r\n
